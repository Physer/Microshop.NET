{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Servicebus": {
    "BaseUrl": "",
    "Port": 5672,
    "ManagementUsername": "",
    "ManagementPassword": ""
  },
  "ReverseProxy": {
    "Routes": {
      "authentication": {
        "ClusterId": "authentication",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "authentication": {
        "Destinations": {
          "authentication": {
            "Address": ""
          }
        }
      }
    }
  },
  "Authentication": {
    "BaseUrl": "",
    "Issuer": "",
    "RelativeJwksEndpoint": "/auth/jwt/jwks.json"
  }
}
