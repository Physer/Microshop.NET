{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Servicebus": {
    "BaseUrl": "",
    "Port": 5672,
    "ManagementUsername": "",
    "ManagementPassword": ""
  },
  "ReverseProxy": {
    "Routes": {
      "indexRoute": {
        "ClusterId": "indexCluster",
        "Match": {
          "Path": "/index/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/index" }
        ]
      }
    },
    "Clusters": {
      "indexCluster": {
        "Destinations": {
          "indexDestination": {
            "Address": ""
          }
        }
      }
    }
  }
}
