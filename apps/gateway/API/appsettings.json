{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "authentication": {
        "ClusterId": "authentication",
        "Match": {
          "Hosts": []
        }
      },
      "admin": {
        "ClusterId": "admin",
        "Match": {
          "Hosts": []
        }
      },
      "api": {
        "ClusterId": "api",
        "Match": {
          "Hosts": []
        }
      }
    },
    "Clusters": {
      "authentication": {
        "Destinations": {
          "authentication": {
            "Address": ""
          }
        }
      },
      "admin": {
        "Destinations": {
          "admin": {
            "Address": ""
          }
        }
      },
      "api": {
        "Destinations": {
          "api": {
            "Address": ""
          }
        }
      }
    }
  }
}
