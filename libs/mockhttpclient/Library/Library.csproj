<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PublicDisplayName>MockHttpClient</PublicDisplayName>
		<AssemblyName>Microshop.$(PublicDisplayName)</AssemblyName>
		<RootNamespace>Microshop.$(PublicDisplayName.Replace(" ", "_"))</RootNamespace>
		<Title>Microshop.NET HttpClient mocking library</Title>
		<Authors>Alex Schouls</Authors>
		<Description>A collection of files useful for mocking HTTP calls</Description>
		<Copyright>© Alex Schouls</Copyright>
		<PackageProjectUrl>https://github.com/Physer/Microshop.NET</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Physer/Microshop.NET</RepositoryUrl>
		<RootNamespace>Microshop.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
		<VersionPrefix>2.0.1</VersionPrefix>
	</PropertyGroup>

</Project>
